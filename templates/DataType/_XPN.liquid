{% if XPN -%}
    {% assign use = XPN."7".Value | get_property: 'CodeSystem/NameType', 'code' -%}
    "family":"{{ XPN."1".Value }}",
    "given":
    [
        "{{ XPN."2".Value }}",
        "{{ XPN."3".Value }}",
        {% if use == 'usual'-%}
        "{{ XPN."15".Value }}",
        {% endif -%}
    ],
    "suffix":
    [
        "{{ XPN."4".Value }}",
        "{{ XPN."6".Value }}",
        "{{ XPN."14".Value }}",
    ],
    "prefix":
    [
        "{{ XPN."5".Value }}",
    ],
    "use":"{{ XPN."7".Value | get_property: 'CodeSystem/NameType', 'code' }}",
    "period":
    {
        "start":"{{ XPN."10"."1" | format_as_date_time }}",
        "start":"{{ XPN."12".Value | format_as_date_time }}",
        "end":"{{ XPN."10"."2" | format_as_date_time }}",
        "end":"{{ XPN."13".Value | format_as_date_time }}",
    },
    {% if use == 'official' %}
        "extension" : [
            {
                "url" : "http://hl7.fr/ig/fhir/core/StructureDefinition/fr-core-patient-birth-list-given-name",
                "valueString" : "{{ XPN."1".Value }} {{ XPN."2".Value }} {{ XPN."3".Value }} {{ XPN."4".Value }}"
            }
        ],
    {% endif -%}
{% endif -%}